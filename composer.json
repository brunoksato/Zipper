{
  "name": "brunoksato/zipper",
  "type": "library",
  "description": "This is a little neat helper for the ZipArchive methods with handy functions",
  "keywords": [
    "laravel",
    "ZIP",
    "Archive"
  ],
  "homepage": "http://github.com/brunoksato/zipper",
  "license": "Apache2",
  "authors": [
    {
      "name": "Bruno Sato",
      "email": "bruno.sato@live.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2||^8.0",
    "ext-zip": "*",
    "illuminate/support": "^6.0||^7.0||^8.0||^9.0||^10.0",
    "illuminate/filesystem": "^6.0||^8.0||^9.0||^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Chumper\\Zipper\\": "src/Chumper/Zipper"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chumper\\Zipper\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Chumper\\Zipper\\ZipperServiceProvider"
      ],
      "aliases": {
        "Zipper": "Chumper\\Zipper\\Zipper"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
